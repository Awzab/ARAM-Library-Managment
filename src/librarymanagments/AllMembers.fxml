<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="rootPane" stylesheets="@../css/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="librarymanagments.AllMembersController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="contentPane" prefHeight="466.0" prefWidth="722.0" stylesheets="@../css/dark-theme.css">
         <children>
            <TableView fx:id="tblMembers" layoutY="27.0" prefHeight="373.0" prefWidth="722.0">
               <columns>
                  <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="colAddress" maxWidth="2800.0" prefWidth="60.0" text="Address" />
                  <TableColumn fx:id="colEmail" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="colMobile" prefWidth="75.0" text="Mobile" />
                  <TableColumn fx:id="colType" prefWidth="75.0" text="Type" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <contextMenu>
                  <ContextMenu styleClass="table-context">
                    
                  </ContextMenu>
               </contextMenu>
            </TableView>
            <MenuBar prefHeight="32.0" prefWidth="722.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#exportAsPDF" text="Export as PDF" />
                        <MenuItem mnemonicParsing="false" onAction="#closeStage" text="Close" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <JFXButton fx:id="backbtn" layoutX="268.0" layoutY="411.0" onAction="#Back" text="BACK" />
            <JFXButton fx:id="btnDelete" layoutX="376.0" layoutY="411.0" onAction="#deleteMember" text="DELETE" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
